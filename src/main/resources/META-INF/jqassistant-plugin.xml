<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.1"
                               name="XML">
    <description>Provides scanners for Source Code (currently Java).</description>
    <model>
        <class>org.unileipzig.jqassistant.plugin.parser.api.model.ModuleDescriptor</class>
        <class>org.unileipzig.jqassistant.plugin.parser.api.model.StatementDescriptor</class>
        <class>org.unileipzig.jqassistant.plugin.parser.api.model.ImportDescriptor</class>
        <class>org.unileipzig.jqassistant.plugin.parser.api.model.DeclarationDescriptor</class>
        <class>org.unileipzig.jqassistant.plugin.parser.api.model.ClassDescriptor</class>
        <class>org.unileipzig.jqassistant.plugin.parser.api.model.AttributeDescriptor</class>
        <class>org.unileipzig.jqassistant.plugin.parser.api.model.MethodDescriptor</class>
    </model>
    <scanner>
        <class>org.unileipzig.jqassistant.plugin.parser.impl.scanner.JavaModuleScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>
