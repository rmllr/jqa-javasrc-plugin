<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.1"
                               name="XML">
    <description>Provides scanners for Source Code (currently Java).</description>
    <model>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.AbstractDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.AccessModifierDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.AnnotatedDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.AnnotationDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.AnnotationTypeDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.AnnotationValueDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.ClassDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.ClassFileDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.ClassTypeDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.ClassValueDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.ConstructorDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.EnumDescriptor</class>
