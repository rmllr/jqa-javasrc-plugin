   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.EnumValueDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.FieldDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.InterfaceTypeDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.InvokesDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.LineNumberDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.MemberDescriptor</class>
   		<class>org.jqassistant.contrib.plugin.javasrc.api.model.MethodDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.PackageDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.PackageMemberDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.ParameterDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.PrimitiveValueDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.ReadsDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.SignatureDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.TypedDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.TypeDependsOnDescriptor</class>        
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.TypeDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.VariableDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.WritesDescriptor</class>       
        <!-- added by DM -->
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.JavaDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.JavaSourceCodeDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.JavaSourceDirectoryDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javasrc.api.model.JavaSourceFileDescriptor</class>
    	<!-- added by DM -->
    </model>
    <scope>
        <class>org.jqassistant.contrib.plugin.javasrc.api.scanner.JavaScope</class>
    </scope>
    <scanner> 
        <class>org.jqassistant.contrib.plugin.javasrc.impl.scanner.JavaSourceDirectoryScannerPlugin</class>
        <class>org.jqassistant.contrib.plugin.javasrc.impl.scanner.JavaSourceFileScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>
